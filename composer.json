{
	"name": "shopavel/demo-products",
	"description": "Shopavel products demo",
	"keywords": ["shopavel", "laravel"],
	"license": "MIT",
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/shopavel/core"
		},
		{
			"type": "vcs",
			"url": "https://github.com/shopavel/products"
		}
	],
	"require": {
		"laravel/framework": "4.0.*",
		"shopavel/core": "dev-master",
		"shopavel/products": "dev-master"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"pre-update-cmd": [
			"php artisan clear-compiled"
		],
		"post-update-cmd": [
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
